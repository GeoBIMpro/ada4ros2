library project Lib@EXT_LIB_NAME@ is
   For externally_built use "true";
   for Languages use ("C");
   for Source_Dirs use ("@CMAKE_INSTALL_PREFIX@/include");
   For library_dir use "@CMAKE_INSTALL_PREFIX@/lib";
   For library_name use "@EXT_LIB_NAME@";
   For library_kind use "dynamic";
end Lib@EXT_LIB_NAME@;